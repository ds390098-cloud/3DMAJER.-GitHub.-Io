export default function ImageVideoGenerator({ user, setUser }) {
  const [prompt, setPrompt] = useState("");
  const [loading, setLoading] = useState(false);

  const generate = async () => {
    if(user.coins < 50){
      alert("Not enough coins! Recharge â‚¹200 = 1000 ðŸª™");
      return;
    }

    setLoading(true);

    // Deduct coins first
    const updatedUser = {...user, coins: user.coins - 50};
    setUser(updatedUser);
    localStorage.setItem("user", JSON.stringify(updatedUser));

    // Call AI APIs
    try {
      // Free Demo Mode
      const imageRes = await fetch("/api/image/free", { method: "POST", body: JSON.stringify({prompt})});
      const imageData = await imageRes.json();

      const voiceRes = await fetch("/api/voice/free", { method: "POST", body: JSON.stringify({text: prompt})});
      const voiceData = await voiceRes.json();

      alert("âœ… Image + Speaking Video ready! (Demo mode)");
      console.log("Image URL:", imageData.url);
      console.log("Voice URL:", voiceData.url);

      // TODO: Combine image + voice into video using FFmpegKit (mobile)
    } catch(e){
      console.error(e);
      alert("Error generating video");
    }

    setLoading(false);
  };

  return (
    <div>
      <h3>Image + Speaking Video (50 coins)</h3>
      <input placeholder="Type prompt here" value={prompt} onChange={e=>setPrompt(e.target.value)}/>
      <button onClick={generate} disabled={loading}>
        {loading ? "Generating..." : "Generate"}
      </button>
      <p>Coins: {user.coins}</p>
    </div>
  );
}// image/free
