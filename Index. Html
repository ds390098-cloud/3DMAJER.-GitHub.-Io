export default function Dashboard() {
  const [coins, setCoins] = useState(0);
  const [firstTime, setFirstTime] = useState(true);

  useEffect(() => {
    // First time bonus
    if (firstTime) {
      setCoins(1000);
      setFirstTime(false);
    }
  }, [firstTime]);

  // Cost logic
  const COST = {
    video: 29,
    image: 20,
    voice: 60,
  };

  const handleGenerate = async (type) => {
    let totalCost = 0;
    if (type === "videoVoice") totalCost = COST.video + COST.voice; // combined generation
    else totalCost = COST[type];

    if (coins >= totalCost) {
      setCoins(coins - totalCost);

      // Call API for generation
      console.log(`${type} generated. Coins deducted: ${totalCost}`);
      alert(`${type} generated. Coins left: ${coins - totalCost}`);
    } else {
      alert(
        `Not enough coins! Please recharge. 200â‚¹ = 1000 coins available.`
      );
    }
  };

  const handleRecharge = () => {
    setCoins(coins + 1000);
    alert("Recharge successful! 1000 coins added.");
  };

  return (
    <div>
      <h2>3D Majer Dashboard</h2>
      <p>Coins: {coins}</p>

      <button onClick={() => handleGenerate("image")}>Generate Image (-20)</button>
      <button onClick={() => handleGenerate("videoVoice")}>Generate Video + Voice (-89)</button>
      <button onClick={() => handleGenerate("voice")}>Generate Voice (-60)</button>

      <button onClick={handleRecharge}>Recharge 200â‚¹ = 1000 Coins</button>
    </div>
  );
}const express = require("express");
const router = express.Router();

// Temporary user coins storage (replace with DB)
let userCoins = 1000;

router.post("/generate", (req, res) => {
  const { type } = req.body;
  let cost = 0;

  if (type === "image") cost = 20;
  else if (type === "voice") cost = 60;
  else if (type === "videoVoice") cost = 29 + 60; // video+voice combined

  if (userCoins >= cost) {
    userCoins -= cost;
    res.json({ success: true, coinsLeft: userCoins });
  } else {
    res.json({ success: false, message: "Not enough coins!" });
  }
});

router.post("/recharge", (req, res) => {
  // 200â‚¹ = 1000 coins
  userCoins += 1000;
  res.json({ success: true, coinsLeft: userCoins });
});

module.exports = router;import React, { useState } from "react";

export default function Login({ setUser }) {
  const [email, setEmail] = useState("");
  const [phone, setPhone] = useState("");

  const handleLogin = () => {
    if (!email && !phone) {
      alert("Enter Email or Phone number to login");
      return;
    }

    // Generate unique ID for this user
    const userId = "user_" + Date.now();

    // First time coins
    const coins = 1000;

    // Store user info in localStorage (for demo)
    const userData = { userId, email, phone, coins };
    localStorage.setItem("3DMajerUser", JSON.stringify(userData));

    setUser(userData);
    alert(`Welcome! You received 1000 coins first time login`);
  };

  return (
    <div>
      <h2>3D Majer Login</h2>
      <input
        type="email"
        placeholder="Enter Gmail"
        value={email}
        onChange={(e) => setEmail(e.target.value)}
      />
      <br />
      <input
        type="text"
        placeholder="Enter Phone Number"
        value={phone}
        onChange={(e) => setPhone(e.target.value)}
      />
      <br />
      <button onClick={handleLogin}>Login</button>
    </div>
  );
}import React, { useState, useEffect } from "react";

export default function Dashboard() {
  const [user, setUser] = useState(null);

  useEffect(() => {
    const storedUser = localStorage.getItem("3DMajerUser");
    if (storedUser) setUser(JSON.parse(storedUser));
  }, []);

  if (!user) return <p>Please login first.</p>;

  const deductCoins = (cost) => {
    if (user.coins >= cost) {
      const newCoins = user.coins - cost;
      const updatedUser = { ...user, coins: newCoins };
      setUser(updatedUser);
      localStorage.setItem("3DMajerUser", JSON.stringify(updatedUser));
      alert(`Coins deducted: ${cost}, remaining: ${newCoins}`);
      return true;
    } else {
      alert("Not enough coins! Recharge 200â‚¹ = 1000 coins");
      return false;
    }
  };

  const handleGenerateImage = () => deductCoins(20);
  const handleGenerateVideoVoice = () => deductCoins(29 + 60);
  const handleGenerateVoice = () => deductCoins(60);
  const handleRecharge = () => deductCoins(-1000); // add coins
  

  return (
    <div>
      <h2>Welcome {user.email || user.phone}</h2>
      <p>Coins: {user.coins}</p>

      <button onClick={handleGenerateImage}>Generate Image (-20)</button>
      <button onClick={handleGenerateVideoVoice}>Generate Video + Voice (-89)</button>
      <button onClick={handleGenerateVoice}>Generate Voice (-60)</button>
      <button
        onClick={() => {
          const newCoins = user.coins + 1000;
          const updatedUser = { ...user, coins: newCoins };
          setUser(updatedUser);
          localStorage.setItem("3DMajerUser", JSON.stringify(updatedUser));
          alert("Recharge successful! 1000 coins added.");
        }}
      >
        Recharge 200â‚¹ = 1000 Coins
      </button>
    </div>
  );
}import React, { useState } from "react";
import Login from "./components/Login";
import Dashboard from "./components/Dashboard";

function App() {
  const [user, setUser] = useState(null);

  return (
    <div className="App">
      {!user ? <Login setUser={setUser} /> : <Dashboard />}
    </div>
  );
}

