export default function Referral({ user, setUser }) {
  const [refId, setRefId] = useState("");

  const applyReferral = async () => {
    if(!refId) return alert("Enter referral Game ID");

    if(user.referredBy) return alert("Already used a referral!");

    // Call backend / firebase
    const refUserSnap = await db.collection("users")
      .where("gameId", "==", refId).get();

    if(refUserSnap.empty) return alert("Invalid referral ID");

    // Add coins to both
    refUserSnap.forEach(doc => {
      doc.ref.update({
        coins: firebase.firestore.FieldValue.increment(250),
        referrals: firebase.firestore.FieldValue.arrayUnion(user.gameId)
      });
    });

    // Add coins to new user
    const userRef = db.collection("users").doc(user.uid);
    userRef.update({
      coins: firebase.firestore.FieldValue.increment(250),
      referredBy: refId
    });

    alert("ðŸŽ‰ Referral applied! 250 coins added to both users");
  };

  return (
    <div>
      <h3>Use Referral (Get 250 coins)</h3>
      <input 
        placeholder="Enter Game ID"
        value={refId}
        onChange={e=>setRefId(e.target.value)}
      />
      <button onClick={applyReferral}>Apply Referral</button>
      <p>Your Coins: {user.coins}</p>
    </div>
  );
}function viewReferrals(){
