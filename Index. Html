}const PLANS = {
  10:   { coins: 50 },
  25:   { coins: 100 },
  190:  { coins: 700 },
  200:  { coins: 1000 },
  500:  { coins: 3000 },
  600:  { coins: 7900, proDays: 7 },
  1000: { coins: 10000, proDays: 30 },
  1200: { coins: 12000, proDays: 37 },

  100:  { proOnly: true, proDays: 30 },
  3000: { unlimited: true, days: 30 },
  10000:{ unlimited: true, lifetime: true }
};function applyPlan(price) {
  const plan = PLANS[price];

  if(plan.coins){
    user.coins += plan.coins;
  }

  if(plan.proDays){
    user.proUntil = Date.now() + plan.proDays * 24 * 60 * 60 * 1000;
  }

  if(plan.proOnly){
    user.proUntil = Date.now() + plan.proDays * 24 * 60 * 60 * 1000;
  }

  if(plan.unlimited){
    user.unlimited = true;
    user.unlimitedUntil = plan.lifetime 
      ? "LIFETIME"
      : Date.now() + plan.days * 24 * 60 * 60 * 1000;
  }

  saveUser(user);
