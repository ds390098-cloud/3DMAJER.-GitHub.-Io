}import React, { useState } from "react";

export default function VoiceToText({ user, setUser }) {
  const [file, setFile] = useState(null);
  const [text, setText] = useState("");
  const [loading, setLoading] = useState(false);

  const handleFile = (e) => setFile(e.target.files[0]);

  const transcribe = async () => {
    if(user.coins < 200){
      alert("Not enough coins! Recharge â‚¹200 = 1000 ðŸª™");
      return;
    }
    if(!file) return alert("Select a video or audio file first!");

    setLoading(true);

    // Deduct coins
    const updatedUser = {...user, coins: user.coins - 200};
    setUser(updatedUser);
    localStorage.setItem("user", JSON.stringify(updatedUser));

    try {
      // Demo / Free mode
      const formData = new FormData();
      formData.append("file", file);

      const res = await fetch("/api/voice-to-text/free", {
        method: "POST",
        body: formData
      });
      const data = await res.json();

      setText(data.text);
      alert("âœ… Voice â†’ Text ready! (Demo Mode)");
    } catch(e){
      console.error(e);
      alert("Error during transcription");
    }

    setLoading(false);
  };

  return (
    <div>
      <h3>Voice â†’ Text (200 coins)</h3>
      <input type="file" accept="audio/*,video/*" onChange={handleFile} />
      <button onClick={transcribe} disabled={loading}>
        {loading ? "Processing..." : "Transcribe"}
      </button>
      <p>Coins: {user.coins}</p>
      <textarea value={text} readOnly rows={10} cols={50}></textarea>
    </div>
  );
}// Demo free transcription
